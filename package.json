{
  "name": "navidonskis.com",
  "version": "1.0.0",
  "description": "<img src=\"https://upload.wikimedia.org/wikipedia/commons/1/11/Flag_of_Lithuania.svg\" width=\"100px\" align=\"right\" alt=\"Lithuania\">",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start_old": "node-sass --watch src/sass/app.scss assets/app.css",
    "start": "npm run build:dev",
    "build:dev": "webpack-dev-server --env dev --mode none",
    "build:prod": "webpack --env prod --mode production",
    "build_sass": "node-sass src/sass/app.scss assets/app.css",
    "build_js": "webpack --mode production",
    "build": "npm run build:prod && npm run build_sass && npm run minify-css",
    "minify-css": "bin/minify.sh css",
    "imagemin": "imagemin assets/images/* --out-dir=assets/images/compressed && imagemin assets/images/* --out-dir=assets/images/webp --plugin=webp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navidonskis/navidonskis.com.git"
